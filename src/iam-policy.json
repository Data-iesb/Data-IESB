{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "qbusiness:Chat",
                "qbusiness:ChatSync",
                "qbusiness:GetApplication",
                "qbusiness:ListApplications"
            ],
            "Resource": [
                "arn:aws:qbusiness:*:*:application/*"
            ]
        },
        {
            "Effect": "Allow",
            "Action": [
                "qbusiness:ListApplications"
            ],
            "Resource": "*"
        }
    ]
}
